{
  "name": "frontend-starter-kit",
  "version": "1.5.0",
  "author": "Eli Van Zoeren <eli@elivz.com>",
  "homepage": "https://elivz.com/",
  "license": "MIT",
  "private": true,
  "main": "gulpfile.js",
  "repository": {
    "type": "git",
    "url": "git@github.com:elivz/FrontendStarterKit.git"
  },
  "build": {
    "paths": {
      "url": "http://localhost:8080",
      "webroot": "./dist/public_html"
    },
    "tasks": {
      "templates": {
        "src": "./src/templates/**/*.{html,php,twig}",
        "dist": "./dist/cms/templates"
      },
      "styles": {
        "src": "./src/scss/**/*.scss",
        "dist": "./dist/public_html/assets/styles"
      },
      "scripts": {
        "files": {
          "main": [
            "./src/js/main.js"
          ],
          "initial": [
            "./src/js/initial.js"
          ]
        },
        "dist": "./dist/public_html/assets/scripts",
        "lint": [
          "!./src/js/plugins/analytics.js",
          "!./src/js/config.js",
          "!./src/js/jspm_packages/**/*",
          "./src/js/**/*.js"
        ]
      },
      "images": {
        "main": {
          "src": [
            "./src/images/**/*.{svg,jpg,jpeg,png,gif,webp}",
            "!./src/images/inline/**/*"
          ],
          "dist": "./dist/public_html/assets/images"
        },
        "inline": {
          "src": "./src/images/inline/**/*.{svg,jpg,jpeg,png,gif,webp}",
          "dist": "./dist/cms/templates/assets"
        },
        "svgo": [
          {
            "removeDimensions": true
          },
          {
            "convertPathData": {
              "floatPrecision": 1
            }
          },
          {
            "cleanupNumericValues": {
              "floatPrecision": 1
            }
          },
          {
            "sortAttrs": true
          },
          {
            "cleanupIDs": false
          }
        ]
      },
      "sprites": {
        "src": "./src/sprites/**/*.svg",
        "dist": "./dist/cms/templates/assets"
      },
      "fonts": {
        "src": "./src/fonts/**/*",
        "dist": "./dist/public_html/assets/fonts"
      },
      "static": {
        "src": "./src/static/**/*",
        "dist": "./dist/public_html"
      }
    },
    "default": [
      "clean",
      [
        "fonts",
        "images",
        "sprites",
        "static"
      ],
      "styles",
      "scripts",
      "templates"
    ],
    "clean": [
      "./dist/cms/templates",
      "./dist/public_html/assets",
      "./dist/public_html/*.{htaccess,html,php,twig,ico,png}"
    ],
    "manifest": {
      "path": "./src",
      "file": "./src/rev-manifest.json"
    }
  },
  "scripts": {
    "watch": "gulp watch",
    "build": "gulp",
    "production": "gulp --production",
    "lint": "gulp scripts:lint"
  },
  "engines": {
    "node": ">=4.6.0"
  },
  "dependencies": {
    "breakpoint-sass": "^2.6.1",
    "sanitize.css": "^5.0.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.4.1",
    "babel-eslint": "^7.1.0",
    "babel-preset-env": "^1.1.1",
    "browser-sync": "^2.14.3",
    "cssnano": "^3.7.4",
    "del": "^2.2.2",
    "eslint": "^3.4.0",
    "eslint-config-airbnb-base": "^11.0.0",
    "eslint-plugin-import": "^2.0.1",
    "gulp": "^3.9.0",
    "gulp-cached": "^1.1.0",
    "gulp-eslint": "^3.0.1",
    "gulp-filter": "^5.0.0",
    "gulp-if": "^2.0.1",
    "gulp-imagemin": "^3.0.3",
    "gulp-jspm": "^0.5.13",
    "gulp-modify": "^0.1.1",
    "gulp-notify": "^3.0.0",
    "gulp-plumber": "^1.0.1",
    "gulp-postcss": "^6.1.1",
    "gulp-rev": "^7.1.2",
    "gulp-sass": "^3.0.0",
    "gulp-sequence": "^0.4.6",
    "gulp-size": "^2.0.0",
    "gulp-sourcemaps": "^2.2.0",
    "gulp-svg-sprite": "^1.3.6",
    "gulp-unretina": "^0.1.3",
    "lazypipe": "^1.0.1",
    "postcss-assets": "^4.1.0",
    "yargs": "^7.0.0"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "browsers": [
              "last 2 versions",
              "> 1%"
            ]
          },
          "modules": false
        }
      ]
    ]
  },
  "eslintConfig": {
    "extends": "airbnb-base",
    "env": {
      "browser": true
    },
    "rules": {
      "import/extensions": 0,
      "import/no-extraneous-dependencies": 0,
      "import/no-unresolved": 0,
      "indent": [
        2,
        4
      ]
    }
  },
  "jspm": {
    "directories": {
      "baseURL": "src/js"
    },
    "dependencies": {
      "a11y-toggle": "npm:a11y-toggle@^2.1.0",
      "jquery": "npm:jquery@3.1.0",
      "lazysizes": "npm:lazysizes@^3.0.0"
    },
    "devDependencies": {
      "babel": "npm:babel-core@^5.8.24",
      "babel-runtime": "npm:babel-runtime@^5.8.24",
      "core-js": "npm:core-js@^1.1.4"
    }
  }
}
