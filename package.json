{
  "name": "frontend-starter-kit",
  "version": "1.2.3",
  "author": "Eli Van Zoeren <eli@elivz.com>",
  "homepage": "https://elivz.com/",
  "license": "MIT",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git@github.com:elivz/FrontendStarterKit.git"
  },
  "engines": {
    "node": ">=4.0.0"
  },
  "scripts": {
    "watch": "gulp watch",
    "build": "gulp",
    "production": "gulp --production",
    "lint": "gulp scripts:lint"
  },
  "dependencies": {},
  "devDependencies": {
    "autoprefixer": "^6.4.0",
    "babel-core": "^6.13.2",
    "babel-eslint": "^6.1.2",
    "babel-preset-es2015": "^6.13.2",
    "breakpoint-sass": "^2.6.1",
    "browser-sync": "^2.14.0",
    "cssnano": "^3.7.3",
    "del": "^2.2.1",
    "eslint": "^3.2.2",
    "eslint-config-airbnb-base": "^5.0.1",
    "eslint-plugin-import": "^1.12.0",
    "gulp": "^3.9.0",
    "gulp-cached": "^1.1.0",
    "gulp-eslint": "^3.0.1",
    "gulp-filter": "^4.0.0",
    "gulp-if": "^2.0.1",
    "gulp-imagemin": "^3.0.2",
    "gulp-jspm": "github:tniswong/gulp-jspm",
    "gulp-modify": "^0.1.1",
    "gulp-notify": "^2.2.0",
    "gulp-plumber": "^1.0.1",
    "gulp-postcss": "^6.1.1",
    "gulp-rename": "^1.2.2",
    "gulp-rev": "^7.1.0",
    "gulp-sass": "^2.3.2",
    "gulp-sequence": "^0.4.4",
    "gulp-size": "^2.0.0",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-svg-sprite": "^1.3.3",
    "gulp-uglify": "^2.0.0",
    "gulp-unretina": "^0.1.3",
    "jspm": "^0.16.42",
    "lazypipe": "^1.0.1",
    "modernizr": "^3.3.1",
    "path": "^0.12.7",
    "postcss-assets": "^4.1.0",
    "postcss-reporter": "^1.4.1",
    "postcss-scss": "^0.1.9",
    "require-dir": "^0.3.0",
    "yargs": "^4.8.1"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "eslintConfig": {
    "extends": "airbnb-base",
    "rules": {
      "import/no-unresolved": 0,
      "indent": [
        2,
        4
      ]
    }
  },
  "stylelint": {
    "extends": "stylelint-config-standard",
    "rules": {
      "indentation": 4,
      "string-no-newline": null,
      "at-rule-empty-line-before": [
        "always",
        {
          "except": [
            "blockless-group"
          ],
          "ignore": [
            "after-comment",
            "all-nested"
          ]
        }
      ],
      "max-nesting-depth": 2,
      "selector-max-specificity": "0,3,1"
    }
  },
  "jspm": {
    "directories": {
      "baseURL": "src/js"
    },
    "dependencies": {
      "autotrack": "npm:autotrack@^0.6.5",
      "jquery": "npm:jquery@3.0.0-beta1",
      "picturefill": "npm:picturefill@^3.0.2",
      "svgxuse": "npm:svgxuse@^1.1.6"
    },
    "devDependencies": {
      "babel": "npm:babel-core@^5.8.24",
      "babel-runtime": "npm:babel-runtime@^5.8.24",
      "core-js": "npm:core-js@^1.1.4"
    }
  }
}
