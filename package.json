{
  "name": "frontend-starter-kit",
  "version": "2.0.0",
  "author": "Eli Van Zoeren <eli@elivz.com>",
  "homepage": "https://elivz.com/",
  "license": "MIT",
  "private": true,
  "main": "gulpfile.js",
  "repository": {
    "type": "git",
    "url": "git@github.com:elivz/FrontendStarterKit.git"
  },
  "scripts": {
    "dev": "yarn run blendid",
    "build": "yarn run blendid -- build"
  },
  "engines": {
    "node": ">=4.6.0"
  },
  "dependencies": {
    "a11y-toggle": "^2.1.0",
    "breakpoint-sass": "^2.6.1",
    "lazysizes": "^3.0.0",
    "sanitize.css": "^5.0.0"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.1",
    "babel-preset-env": "^1.5.1",
    "blendid": "^4.1.1",
    "gulp-postcss": "^7.0.0",
    "postcss-assets": "^4.1.0"
  },
  "target": {
    "browsers": [
      "last 2 versions",
      "> 1%"
    ]
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "modules": false
        }
      ]
    ]
  },
  "eslintConfig": {
    "env": {
      "browser": true
    },
    "extends": [
      "airbnb-base"
    ],
    "rules": {
      "import/extensions": 0,
      "import/no-extraneous-dependencies": 0,
      "import/no-unresolved": 0,
      "indent": [
        2,
        4
      ],
      "comma-dangle": [
        "error",
        {
          "arrays": "always-multiline",
          "objects": "always-multiline",
          "imports": "always-multiline",
          "exports": "always-multiline",
          "functions": "never"
        }
      ],
      "arrow-parens": [
        "error",
        "as-needed"
      ]
    }
  }
}
