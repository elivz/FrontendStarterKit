    "babel-preset-env": "^1.5.1",
    "blendid": "https://github.com/elivz/blendid.git",
    "eslint": "^3.9.0",
    "eslint-plugin-import": "^2.6.0",
    "eslint-plugin-prettier": "^2.1.2",
    "gulp-eslint": "^4.0.0",
    "gulp-postcss": "^7.0.0",
    "postcss-assets": "^4.1.0",
    "postcss-normalize": "^4.0.0",
    "prettier": "^1.5.2"
  },
  "target": {
